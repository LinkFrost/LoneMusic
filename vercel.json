{
   "version": 2,
   "builds": [
      {
         "src": "./src/server.js",
         "use": "@vercel/node"
      },
      {
         "src": "./client",
         "use": "@vercel/static"
      }
   ],

   "rewrites": [
      {
         "source": "/(.*)",
         "destination": "./src/server.js"
      }
   ]
}
